{
  "name": "no-drainer",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ts-node ./scripts/set-dev-env && ng serve",
    "start:prod": "ng serve --configuration production",
    "build": "ts-node ./scripts/set-prod-env && ts-node ./scripts/version && ng build",
    "build:watch": "ng build --watch --configuration development",
    "build:stats": "ng build --configuration local-stats",
    "test": "ng test",
    "serve:ssr:no-drainer": "node dist/no-drainer/server/server.mjs",
    "deploy": "ts-node ./scripts/deploy --stage prod",
    "lint": "ng lint",
    "bundle-report": "npm run build:stats && webpack-bundle-analyzer dist/no-drainer/stats.json",
    "source-map-explorer": "source-map-explorer dist/no-drainer/browser/main.js dist/no-drainer/browser/main.js.map"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.1.0",
    "@angular/cdk": "^18.1.4",
    "@angular/common": "^18.1.0",
    "@angular/compiler": "^18.1.0",
    "@angular/core": "^18.1.0",
    "@angular/forms": "^18.1.0",
    "@angular/material": "^18.1.4",
    "@angular/platform-browser": "^18.1.0",
    "@angular/platform-browser-dynamic": "^18.1.0",
    "@angular/platform-server": "^18.1.0",
    "@angular/router": "^18.1.0",
    "@angular/ssr": "^18.1.4",
    "@ng-web-apis/common": "^4.2.1",
    "@ng-web-apis/universal": "^4.2.1",
    "@ngspot/ngx-errors": "^4.0.0",
    "@ngspot/ngx-errors-material": "^4.0.1",
    "angulartics2": "^14.1.0",
    "contentful": "^11.0.2",
    "express": "^4.18.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.1.4",
    "@angular-eslint/builder": "18.3.1",
    "@angular-eslint/eslint-plugin": "18.3.1",
    "@angular-eslint/eslint-plugin-template": "18.3.1",
    "@angular-eslint/schematics": "18.3.1",
    "@angular-eslint/template-parser": "18.3.1",
    "@angular/cli": "^18.1.4",
    "@angular/compiler-cli": "^18.1.0",
    "@types/cli-progress": "^3.11.6",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.1.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "7.11.0",
    "@typescript-eslint/parser": "7.11.0",
    "aws-sdk": "^2.1691.0",
    "cli-progress": "^3.12.0",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jasmine-core": "~5.1.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "mime-types": "^2.1.35",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.10",
    "ts-node": "^10.9.2",
    "typescript": "~5.5.2",
    "webpack-bundle-analyzer": "^4.10.2",
    "yargs": "^17.7.2"
  }
}
