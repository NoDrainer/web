{
  "name": "no-drainer",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start.prod": "ng serve --prod --aot",
    "build.prod": "ng build --prod --aot",
    "build.dev": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "aws.deploy": "aws s3 sync dist/ s3://www.nodrainer.com --delete --cache-control max-age=864000 --region us-east-1",
    "aws.invalidate": "aws cloudfront create-invalidation --distribution-id E3DPWJR8FSHWWK --paths /",
    "deploy": "npm run build.prod && npm run aws.deploy && npm run aws.invalidate",
    "prerender": "ng-render",
    "gulp": "gulp",
    "server": "ng build --prod && ngc && ts-node src/server.ts",
    "ngc": "ngc",
    "node": "ts-node src/server.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "4.1.1",
    "@angular/common": "4.1.1",
    "@angular/compiler": "4.1.1",
    "@angular/core": "4.1.1",
    "@angular/flex-layout": "2.0.0-beta.8",
    "@angular/forms": "4.1.1",
    "@angular/http": "4.1.1",
    "@angular/material": "2.0.0-beta.3",
    "@angular/platform-browser": "4.1.1",
    "@angular/platform-browser-dynamic": "4.1.1",
    "@angular/router": "4.1.1",
    "@ultimate/ngxerrors": "^1.3.0",
    "angulartics2": "^2.2.0",
    "core-js": "^2.4.1",
    "rxjs": "^5.3.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@angular/cli": "^1.0.1",
    "@angular/compiler-cli": "4.1.1",
    "@angular/platform-server": "4.1.1",
    "@angular/service-worker": "^1.0.0-beta.10",
    "@nguniversal/express-engine": "^1.0.0-beta.0",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "angular-ssr": "^0.1.97",
    "codelyzer": "~3.0.1",
    "gulp": "^3.9.1",
    "jasmine-core": "~2.6.1",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.6.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.1",
    "ts-node": "~3.0.2",
    "tslint": "~5.1.0",
    "typescript": "~2.3.2"
  }
